							"pm.test(\"should have 29 items\", function () {",
							"    pm.expect(jsonData).to.have.lengthOf(29);",
							"});",
							"",
							"pm.test(\"each item should have port id\", function () { ",
							"        pm.expect(jsonData[0].portId).is.to.equal('8202'); ",
							"    });",
